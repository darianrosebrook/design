{
  "name": "@paths-design/svg-import",
  "version": "0.1.0",
  "description": "SVG import and conversion to canvas nodes for Designer",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
