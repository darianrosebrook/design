{
  "name": "designer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "pnpm -r run typecheck",
    "build": "pnpm -r run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:packages": "pnpm -r run lint",
    "start:mcp": "pnpm --filter @paths-design/mcp-adapter run start",
    "start:websocket": "pnpm --filter @paths-design/websocket-server run start",
    "start:servers": "concurrently \"pnpm run start:mcp\" \"pnpm run start:websocket\"",
    "dev:extension": "echo 'Open packages/vscode-ext in VS Code and press F5'",
    "verify": "pnpm run typecheck && pnpm run lint && pnpm run test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@types/node": "^24.6.2",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "fast-check": "^4.3.0",
    "tsx": "^4.20.6",
    "typescript-eslint": "^8.45.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "ulid": "^3.0.1"
  }
}
